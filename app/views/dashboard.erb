<div class="titlebar row">
  <div class="col-md-10 col-xs-9">
    <h1>Dashboard</h1>
    <p class="subtitle">Your connected projects. Manage your running behaviors here.</p>
  </div>
  <div class="col-md-2 col-xs-3">
    <a href="/connect" class="btn btn-primary">Connect Project</a>
  </div>
</div>

<div class="dashboard">
  <ol class="repos">
    <% @repos.each do |repo| %>
      <%= partial :repo, locals: {repo: repo, active: false} %>
    <% end %>
  </ol>
</div>
